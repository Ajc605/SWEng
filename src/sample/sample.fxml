   <children>
      <ImageView fitHeight="529.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@MediaSweng/forest_path_anime_background_by_wbd-d3l83r9.jpg" />
         </image>
      </ImageView>
      <BorderPane prefHeight="527.0" prefWidth="675.0">
         <bottom>
            <AnchorPane BorderPane.alignment="CENTER">
               <children>
                  <Button layoutX="20.0" layoutY="150.0" mnemonicParsing="false" style="-fx-background-color: transparent; -fx-text-fill: transparent;" text="Files">
                     <font>
                        <Font name="Chalkboard SE Bold" size="18.0" />
                     </font></Button>
                  <Button layoutX="360.0" layoutY="190.0" mnemonicParsing="false" onAction="#childLockPassword" prefHeight="64.0" prefWidth="61.0" style="-fx-background-color: transparent; -fx-text-fill: transparent;" text="Lock">
